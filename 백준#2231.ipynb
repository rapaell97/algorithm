    "num=int(input())\n",
    "\n",
    "for i in range(1,num+1):\n",
    "    num_list=list(map(int,str(i)))\n",
    "    num_sum=sum(num_list)\n",
    "\n",
    "    if num == i+num_sum:\n",
    "        print(i)\n",
    "        break\n",
    "    if num == i:\n",
    "        print(0)\n"
