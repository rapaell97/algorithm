    "test = int(input())\n",
    "info_list=list()\n",
    "\n",
    "for i in range(test):\n",
    "    w,h=list(map(int,input().split()))\n",
    "    info_list.append((w,h))\n",
    "\n",
    "for k in info_list:\n",
    "    cnt=1\n",
    "    for j in info_list:\n",
    "        if k[0]<j[0] and k[1]<j[1]:\n",
    "            cnt+=1\n",
    "    print(cnt,end=' ')\n",
    "\n",
    "\n",
    "\n",
