    "import string\n",
    "\n",
    "alpha_list = [i for i in string.ascii_lowercase]\n",
    "alpha_dict = {alpha:num+1 for num,alpha in enumerate(alpha_list)}\n",
    "\n",
    "test=int(input())\n",
    "word=input()\n",
    "k=0\n",
    "\n",
    "for m,n in enumerate(word):\n",
    "    k+=alpha_dict[n]*(31**m)\n",
    "\n",
    "print(k % 1234567891)\n",
    "\n"
