    "import math\n",
    "\n",
    "test=int(input())\n",
    "test_fac=math.factorial(test)\n",
    "\n",
    "str_test=str(test_fac)\n",
    "r_str_test=str_test[::-1]\n",
    "cnt=0\n",
    "\n",
    "for i in r_str_test:\n",
    "    if i == '0':\n",
    "        cnt+=1\n",
    "    else:\n",
    "        print(cnt)\n",
    "        break\n",
